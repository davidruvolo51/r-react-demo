{
  "name": "api",
  "version": "1.0.0",
  "scripts": {
    "install": "npm install && cd client && npm install",
    "upgrade": "npm upgrade && cd client && npm upgrade",
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "@fridgerator/r-script": "^0.1.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
